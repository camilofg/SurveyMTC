        </formElements>
        <formElements xsi:type="html:Input" name="3" maxLength="10">
          <label content="C&#xf3;digo" forText="3"/>
        </formElements>
      </sections>
      <sections title="Evaluaci&#xf3;n del curso" id="//@surveys.0/@sections.1">
        <formElements xsi:type="html:Select" name="4" required="true" type="COMBO">
          <label content="&#xbf;A qu&#xe9; curso acudes? " forText="4"/>
          <options content="Automatizaci&#xf3;n" value="A3"/>
          <options content="Agiles" value="A4"/>
          <options content="Fabricas de Software" value="A5"/>
        </formElements>
        <formElements xsi:type="html:Select" name="5" required="true" multiple="true">
          <label id="" content="&#xbf;Cu&#xe1;les son los aspectos que m&#xe1;s rescata del curso?" forText="5"/>
