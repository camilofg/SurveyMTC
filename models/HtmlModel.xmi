      <graphs title="Eval&#xfa;a los siguientes enunciados seg&#xfa;n tu opini&#xf3;n"/>
    </views>
    <views title="Encuesta">
      <sections title="Datos personales" id="//@surveys.0/@sections.0">
        <formElements xsi:type="html:Input" name="1" maxLength="50">
          <label content="Nombres" forText="1"/>
        </formElements>
        <formElements xsi:type="html:Select" name="2" required="true">
          <label content="G&#xe9;nero" forText="2"/>
