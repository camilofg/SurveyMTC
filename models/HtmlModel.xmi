          <options content="Tiempo" value="7"/>
          <options content="Contenido del curso" value="8"/>
          <options content="Horario" value="9"/>
          <options content="Plan de estudios" value="10"/>
        </formElements>
        <formElements xsi:type="html:Input" name="6" required="true" type="RANGE" min="1" max="5" step="1">
          <label content="Indique su nivel de satisfacci&#xf3;n general frente al curso" forText="6"/>
        </formElements>
        <formElements xsi:type="html:TextArea" name="7" rows="5" maxLength="100">
          <label content="&#xbf;Hay algo que se podr&#xed;a mejorar en el curso? Por favor, d&#xe9;janos saber tu opini&#xf3;n" forText="7"/>
        </formElements>
        <formElements xsi:type="html:SelectComplex" name="8">
          <label content="Eval&#xfa;a los siguientes enunciados seg&#xfa;n tu opini&#xf3;n" forText="8"/>
          <columnsOptions content="Si" value="1"/>
          <columnsOptions content="No" value="2"/>
          <columnsOptions content="Alguna Veces" value="3"/>
          <options content="&#xbf;Hay cumplimiento del curso?" value="11"/>
          <options content="&#xbf;Existen asesor&#xed;as?" value="12"/>
          <options content="&#xbf;Se resuelven los problemas a tiempo?" value="13"/>
          <options content="&#xbf;Es clara la informaci&#xf3;n suministrada?" value="14"/>
        </formElements>
      </sections>
